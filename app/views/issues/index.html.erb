<h1>Listing issues</h1>
<%= form_tag import_issues_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import", :class => 'small button' %>
<% end %>

<ul>
  <% @issues.each do |issue| %>
    <li><%= link_to "Год #{issue.year}, Т. #{issue.volume}, № #{issue.number}",issue %>
  <% end %>
</ul>

<br>
<%= link_to 'New Issue', new_issue_path %>
